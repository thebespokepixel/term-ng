{
  "name": "term-ng",
  "version": "0.6.3",
  "description": "Terminal/$TERM feature snooping and whitelisting",
  "main": "index.js",
  "bin": {
    "termng": "./bin/termng.js"
  },
  "files": [
    "index.js",
    "lib",
    "bin"
  ],
  "scripts": {
    "test": "xo && ava",
    "colors": "scripts/colortest.pl"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MarkGriffiths/term-ng.git"
  },
  "engines": {
    "node": ">=4.0 <7.0"
  },
  "keywords": [
    "24bit",
    "color",
    "ansi",
    "truecolor",
    "trucolor",
    "sgr",
    "cli",
    "tty",
    "iterm",
    "xterm"
  ],
  "author": "Mark Griffiths <mark@thebespokepixel.com> (http://thebespokepixel.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MarkGriffiths/term-ng/issues"
  },
  "homepage": "https://github.com/MarkGriffiths/term-ng#readme",
  "devDependencies": {
    "@thebespokepixel/cordial": "*",
    "ava": "^0.15.2",
    "gulp": "github:gulpjs/gulp#4.0",
    "shelljs": "^0.7.0",
    "xo": "^0.16.0"
  },
  "dependencies": {
    "core-js": "*",
    "deep-assign": "^2.0.0",
    "read-pkg-up": "^2.0.0",
    "supports-color": "^3.1.2",
    "trucolor": "*",
    "truwrap": "*",
    "update-notifier": "^1.0.0",
    "yargs": "^4.4.0"
  },
  "xo": {
    "semicolon": false,
    "esnext": true,
    "ignores": [
      "lib/**/*.js",
      "bin/*.js"
    ]
  },
  "ava": {
    "verbose": true,
    "files": [
      "test/*.js"
    ]
  },
  "buildNumber": 0,
  "jsnext:main": "lib/index-es6.js"
}
